{
  "name": "puzzle-place",
  "version": "0.1.0",
  "description": "A modern web application for creating and managing game events with prize pools",
  "private": true,
  "scripts": {
    "dev": "concurrently \"bun run dev:next\" \"bun run ws:start\"",
    "dev:next": "next dev --webpack",
    "build": "next build --webpack",
    "postinstall": "bun prisma generate",
    "start": "next start",
    "lint": "eslint",
    "db:seed": "tsx prisma/seed.ts",
    "ws:start": "bun run websocket-server.ts",
    "dev:auto": "node dev-start.js",
    "dev:only": "next dev",
    "dev:full": "concurrently \"bun run dev:next\" \"bun run ws:start\"",
    "dev:full:windows": "start /b bun run dev:next && start /b bun run ws:start"
  },
